<?xml version="1.0" encoding="UTF-8"?>
<openerp>
<data noupdate="1">

    <record id="ir_cron_accredit_issued_checks" model="ir.cron">
        <field name="name">Accredit Issued Checks</field>
        <field name="user_id" ref="base.user_root"/>
        <field name="active" eval="False"/>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
        <field name="model">account.issued.check</field>
        <field name="function">accredit_checks_cron_task</field>
        <field name="priority">10</field>
        <field name="nextcall" eval="time.strftime('%Y-%m-%d 23:59:50')"/>
        <field name="args">()</field>
    </record>

</data>
</openerp>
